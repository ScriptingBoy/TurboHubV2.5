main.Visible = false
end)


open.Name = "OPEN"
open.Parent = main
open.BackgroundColor3 = Color3.new(0, 0, 0)
open.Size = UDim2.new(0, 129, 0, 48)
open.Font = Enum.Font.GothamBold
open.Text = "Open"
open.TextColor3 = Color3.new(0, 0, 0)
